  <!-- INTRO -->
  <section class="intro">

    <div class="row">

      <div class="medium-12 columns">

        <h1>Designer &amp; Developer</h1>
        <p>Hi! My name is Pete Mitchell, I'm a Web Developer in Seattle. <br>I'm
          currently honing my skills at <a href="http://www.codefellows.org">Code
          Fellows</a> and looking for opportunities to contribute to a team.<br>
          I primarily work in Ruby on Rails, with knowledge of front-end tools
          like Javascript and HTML 5.</p>

        <p><a href="#contact" class="button small alert radius">Drop Me A Line</a></p>

        <!-- Start: Slider -->
        <div class="csslider">

          <input type="radio" name="slides" id="slides_1" checked>
          <input type="radio" name="slides" id="slides_2">
          <ul>
            <li><img src="img/Ruby_on_Rails.png" alt=""></li>
            <li><img src="img/codefellows.png" alt=""></li>
          </ul>

          <!-- Buttons Control -->
          <div class="navigation">

            <div>
              <label for="slides_1"></label>
              <label for="slides_2"></label>
            </div>

          </div><!-- /.navigation -->

        </div><!-- /.csslider -->
        <!-- End: Slider -->

      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->

  </section><!-- /.intro -->
  <!-- INTRO -->


  <!-- ABOUT -->
  <section id="about" class="about-wrapper">

    <div class="row">

      <div class="medium-12 columns">

        <h3>About Me</h3>

        <div class="line"></div>
        <div class="clearfix"></div>

      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->

    <div class="col-progress">

      <div class="row">

        <div class="medium-4 columns">

          <h6>Ruby</h6>
          <div class="progress success round">
            <span class="meter" style="width: 90%"></span>
          </div><!-- /.progress -->

          <h6>Rails</h6>
          <div class="progress success round">
            <span class="meter" style="width: 90%"></span>
          </div><!-- /.progress -->

          <h6>HTML 5</h6>
          <div class="progress round">
            <span class="meter" style="width: 80%"></span>
          </div><!-- /.progress -->

        </div><!-- /.medium-4 .columns -->

        <div class="medium-4 columns text-center">

          <div class="col-desc">
            <p>I grew up at the northern end of California's Central Valley,
              in the suburbs surrounding Sacramento. A born extrovert, I filled
              my youth with science fiction books and punk rock music. Despite
              being a computer hobbyist, I choose to go into sales and customer
              service. After 10+ years, I finally decided to put my natural
              ability and passion for programming into full gear.</p>

            <img src="img/Pete-Headshot.jpg" alt="">
          </div><!-- ./col-desc -->

        </div><!-- /.medium-4 .columns -->

        <div class="medium-4 columns">

          <h6>Git/Github</h6>
          <div class="progress medium-12 round">
            <span class="meter" style="width: 90%"></span>
          </div><!-- /.progress -->

          <h6>Javascript</h6>
          <div class="progress medium-12 alert round">
            <span class="meter" style="width: 70%"></span>
          </div><!-- /.progress -->

          <h6>Angular.js &amp; jQuery</h6>
          <div class="progress medium-12 alert round">
            <span class="meter" style="width: 70%"></span>
          </div><!-- /.progress -->

        </div><!-- /.medium-4 .columns -->

      </div><!-- /.row -->

    </div><!-- /.col-progress -->

  </section><!-- /.about-wrapper -->
  <!-- ABOUT -->




  <!-- LATEST TWEETS -->
  <section class="tweets-wrapper">

    <div class="row">

      <div class="medium-12 columns">

        <img src="img/twitter-bird.png" alt="" class="pulse">

        <div id="tweets"></div>
        <script>
          window.onload = function(){
            twitterFetcher.fetch(
              '513871477410525184',     // replace to your id twitter app
              'tweets',           // div id
              1,                // num tweets
              true,             // set true if you want urls and hash
              false,              // Set false if you dont want user photo
              false,              // Set false if you dont want time of tweet to show.
              d,                // Format tweet date
              false,              // Show retweets or not.
              d,                // function to call when data is ready.
              false             // Show links for reply, retweet, favourite.
            );
            var d = function(){return null};  // nothing function
          }
        </script>
        <script src="js/tweets.js"></script>

        <div class="clearfix"></div>
        <p><a href="http://www.twitter.com/petemmitchell" class="button tiny radius">Follow @petemmmitchell</a></p>

      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->

  </section><!-- /.tweets-wrapper -->
  <!-- LATEST TWEETS -->




  <!-- CONTACT -->
  <section id="contact" class="contact-wrapper">

    <div class="row">

      <div class="medium-12 columns">

        <h3>Let's Talk</h3>

        <div class="line"></div>
        <div class="clearfix"></div>

      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->

    <div class="row">

      <div class="medium-3 columns">&nbsp;</div><!-- /.medium-3 .columns -->
      <div class="medium-6 columns">

        <form class="contact-form">
          <%= form_tag(send_mail_path) do %>
            <div class="input-item">
              <label>Name <span class="required">*</span></label>
              <input type="text" placeholder="Full name">
            </div><!-- /.input-item -->

            <div class="input-item">
              <label>Email <span class="required">*</span></label>
              <input type="email" placeholder="Email address">
            </div><!-- /.input-item -->

            <div class="input-item">
              <label>Messages <span class="required">*</span></label>
              <textarea cols="10" rows="4" placeholder="Your message"></textarea>
            </div><!-- /.input-item -->

            <div class="input-item text-center">
              <input class="button small success radius" type="submit" value="Submit">
            </div><!-- /.input-item -->
          <=2
        </form><!-- /.contact-form -->

      </div><!-- /.medium-6 .columns -->
      <div class="medium-3 columns">&nbsp;</div><!-- /.medium-3 .columns -->

    </div><!-- /.row -->

    <div class="clearfix"></div>
    <div class="row margin-top">

      <div class="medium-12 columns">

        <h3>Stay In Touch!</h3>

        <div class="line"></div>
        <div class="clearfix"></div>

        <ul class="social-icon">
          <!-- <li><a href="#" class="facebook"><i class="fa fa-facebook"></i></a></li> -->
          <li><a href="https://twitter.com/petemmitchell" class="twitter"><i class="fa fa-twitter"></i></a></li>
          <li><a href="#" class="google-plus"><i class="fa fa-google-plus"></i></a></li>
          <li><a href="#" class="dribbble"><i class="fa fa-dribbble"></i></a></li>
          <li><a href="#" class="flickr"><i class="fa fa-flickr"></i></a></li>
        </ul><!-- /.social-icon -->

      </div><!-- /.medium-12 .columns -->

    </div><!-- /.row -->

  </section><!-- /.contact-wrapper -->
